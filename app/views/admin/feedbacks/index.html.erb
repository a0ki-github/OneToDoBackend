<p style="color: green"><%= notice %></p>

<h1>Feedbacks</h1>

<div id="feedbacks">
  <% @feedbacks.each do |feedback| %>
    <%= render feedback %>
    <p>
      <%= link_to "Show this feedback", [:admin, feedback] %>
    </p>
  <% end %>
</div>

<%= link_to "New feedback", new_admin_feedback_path %>
